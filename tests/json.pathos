json_value[JsonValue]: obj=json_object -> Object { obj }
    | arr=json_array -> Array { array: arr }
    | s=STRING_LITERAL -> String { value: s }
    | n=NUMBER_LITERAL -> Number { value: n }
    | "true" -> Boolean { value: { True } }
    | "false" -> Boolean { value: { False } }
    | "null" -> Null { };

json_object[dict[str, JsonValue]]: '{' '}' -> { dict() }
    | '{' items=members '}' -> { dict(items) };

members[list[tuple[str, JsonValue]]]: val=member -> { [val] }
    | vals=(member)','+ -> { vals };

member[tuple[str, JsonValue]]: key=STRING_LITERAL ':' value=json_value -> { (key, value) };

json_array[list[JsonValue]]: '[' ']' -> { [] }
    | '[' vals=elements ']' -> { vals };

elements[list[JsonValue]]: vals=(json_value)','+ -> { vals };



